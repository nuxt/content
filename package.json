{
  "name": "@nuxt/content",
  "version": "2.0.0",
  "description": "Write your content inside your Nuxt app",
  "keywords": [
    "nuxt",
    "content",
    "module"
  ],
  "repository": "nuxt/content-next",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prepack": "yarn build",
    "test:benchmark": "node --loader ts-node/esm test/benchmarks/run.ts test/benchmarks/*.benchmark.*",
    "test:coverage": "vitest --coverage",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "nuxi prepare test/fixtures/basic && vitest run"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "consola": "^2.15.3",
    "defu": "^6.0.0",
    "detab": "^3.0.1",
    "flat": "^5.0.2",
    "gray-matter": "^4.0.3",
    "html-tags": "^3.1.0",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-to-hast": "^12.1.1",
    "mdast-util-to-markdown": "^1.3.0",
    "mdurl": "^1.0.1",
    "micromark": "^3.0.10",
    "micromark-core-commonmark": "^1.0.6",
    "micromark-factory-space": "^1.0.0",
    "micromark-factory-whitespace": "^1.0.0",
    "micromark-util-character": "^1.1.0",
    "ohash": "^0.1.0",
    "parse-entities": "^4.0.0",
    "pathe": "^0.2.0",
    "perfect-debounce": "^0.1.3",
    "property-information": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-sort-attribute-values": "^4.0.0",
    "rehype-sort-attributes": "^4.0.0",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-squeeze-paragraphs": "^5.0.1",
    "scule": "^0.2.1",
    "shiki-es": "^0.1.2",
    "slugify": "^1.6.5",
    "stringify-entities": "^4.0.2",
    "ufo": "^0.8.1",
    "unctx": "^1.1.3",
    "unified": "^10.1.2",
    "unist-builder": "^3.0.0",
    "unist-util-position": "^4.0.2",
    "unist-util-visit": "^4.1.0",
    "unist-util-visit-parents": "^5.1.0",
    "unstorage": "^0.3.3",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@nuxt/module-builder": "^0.1.7",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-edge@latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/benchmark": "^2.1.1",
    "@types/debounce": "^1.2.1",
    "@types/flat": "^5.0.2",
    "@types/ws": "^8.5.3",
    "benchmark": "^2.1.4",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "globby": "^13.1.1",
    "husky": "^7.0.4",
    "jiti": "^1.13.0",
    "lint-staged": "^12.3.7",
    "nuxt3": "latest",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vitest": "^0.7.12"
  }
}
