{
  "name": "nuxt-content",
  "private": true,
  "description": "Write your content inside your Nuxt app",
  "repository": "nuxt/content",
  "license": "MIT",
  "contributors": [
    {
      "name": "NuxtJS Team"
    }
  ],
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "dev": "nuxt example",
    "docs": "nuxt docs",
    "lint": "eslint --ext .js,.vue .",
    "release": "yarn test && lerna publish",
    "test": "yarn lint && yarn test:packages",
    "test:watch": "jest packages --watch",
    "test:packages": "jest packages -i",
    "test:packages:content": "jest packages/content -i",
    "test:packages:create-nuxt-content-docs": "jest packages/create-nuxt-content-docs"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "codecov": "^3.7.2",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lerna": "^3.22.1",
    "nuxt": "^2.14.3",
    "puppeteer": "^5.2.1",
    "tib": "^0.7.4"
  }
}