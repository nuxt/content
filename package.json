{
  "name": "nuxt-content",
  "private": true,
  "description": "Write your content inside your Nuxt app",
  "repository": "nuxt/content",
  "license": "MIT",
  "contributors": [
    {
      "name": "NuxtJS Team"
    }
  ],
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "dev": "nuxt-ts example",
    "docs": "nuxt docs",
    "lint": "eslint --ext .js,.vue .",
    "release": "yarn test && lerna publish",
    "test": "yarn lint && yarn test:packages",
    "test:watch": "jest packages --watch",
    "test:packages": "jest packages -i",
    "test:packages:content": "jest packages/content -i",
    "test:packages:create-nuxt-content-docs": "jest packages/create-nuxt-content-docs"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "codecov": "^3.8.1",
    "eslint": "^7.13.0",
    "highlight.js": "^10.4.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "nuxt": "^2.14.7",
    "puppeteer": "^5.5.0",
    "shiki": "^0.2.7",
    "shiki-twoslash": "^0.8.1",
    "tib": "^0.7.4"
  }
}
