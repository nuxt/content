{
  "name": "@nuxt/content",
  "version": "3.0.0-alpha.8",
  "description": "Write your content inside your Nuxt app",
  "homepage": "https://content.nuxt.com",
  "keywords": [
    "nuxt",
    "content",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/content.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./studio": {
      "types": "./dist/studio.d.ts",
      "import": "./dist/studio.mjs",
      "require": "./dist/studio.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "typecheck": "nuxi typecheck",
    "example": "run () { nuxi dev examples/$*; }; run",
    "docs": "nuxi dev docs",
    "docs:build": "nuxi build docs",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && release-it",
    "lint": "eslint .",
    "test": "vitest run",
    "test:bun": "bun test ./test/bun.test.ts",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "verify": "npm run dev:prepare && npm run lint && npm run test && npm run typecheck"
  },
  "dependencies": {
    "@nuxtjs/mdc": "^0.12.1",
    "@shikijs/langs": "^1.26.1",
    "@sqlite.org/sqlite-wasm": "3.47.2-build1",
    "better-sqlite3": "^11.7.2",
    "c12": "^2.0.1",
    "chokidar": "^4.0.3",
    "consola": "^3.3.3",
    "defu": "^6.1.4",
    "destr": "^2.0.3",
    "fast-glob": "^3.3.3",
    "git-url-parse": "^16.0.0",
    "jiti": "^2.4.2",
    "knitwork": "^1.2.0",
    "listhen": "^1.9.0",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark": "^4.0.1",
    "micromark-util-character": "^2.1.1",
    "micromark-util-chunked": "^2.0.1",
    "micromark-util-resolve-all": "^2.0.1",
    "micromark-util-sanitize-uri": "^2.0.1",
    "micromatch": "^4.0.8",
    "minimatch": "^10.0.1",
    "nuxt-component-meta": "^0.9.0",
    "ohash": "^1.1.4",
    "parse-git-config": "^3.0.0",
    "pathe": "^2.0.0",
    "pkg-types": "^1.3.0",
    "remark-mdc": "latest",
    "scule": "^1.3.0",
    "shiki": "^1.26.1",
    "slugify": "^1.6.6",
    "socket.io-client": "^4.8.1",
    "tar": "^7.4.3",
    "typescript": "5.7.2",
    "ufo": "^1.5.4",
    "unified": "^11.0.5",
    "unist-util-stringify-position": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1",
    "zod-to-ts": "^1.2.0"
  },
  "peerDependencies": {
    "@libsql/client": "*",
    "pg": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241230.0",
    "@nuxt/content": "link:.",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.4",
    "@nuxt/kit": "^3.15.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.1",
    "@nuxt/test-utils": "^3.15.1",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.1.15",
    "@types/micromatch": "^4.0.9",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.13",
    "csvtojson": "^2.0.10",
    "eslint": "^9.17.0",
    "mdclint": "^0.0.2",
    "micromark-util-types": "^2.0.1",
    "nuxt": "^3.15.1",
    "release-it": "^18.0.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0",
    "wrangler": "^3.100.0"
  },
  "resolutions": {
    "@nuxt/content": "workspace:*",
    "unimport": "3.14.5"
  },
  "packageManager": "pnpm@9.15.3",
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/studio"
    ],
    "externals": [
      "untyped",
      "bun:sqlite",
      "bun:test"
    ],
    "rollup": {
      "output": {
        "exports": "named"
      },
      "emitCJS": true
    }
  }
}
