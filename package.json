{
  "name": "@nuxt/content",
  "version": "3.0.0-alpha.6",
  "description": "Write your content inside your Nuxt app",
  "homepage": "https://content.nuxt.com",
  "keywords": [
    "nuxt",
    "content",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/content.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "docs": "nuxi dev docs",
    "docs:build": "nuxi build docs",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && release-it",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.159",
    "@nuxtjs/mdc": "^0.9.2",
    "@sqlite.org/sqlite-wasm": "3.47.0-build1",
    "better-sqlite3": "^11.5.0",
    "c12": "^2.0.1",
    "chokidar": "^4.0.1",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "destr": "^2.0.3",
    "fast-glob": "^3.3.2",
    "jiti": "^2.4.0",
    "knitwork": "^1.1.0",
    "listhen": "^1.9.0",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark": "^4.0.0",
    "micromark-util-character": "^2.1.0",
    "micromark-util-chunked": "^2.0.0",
    "micromark-util-resolve-all": "^2.0.0",
    "micromark-util-sanitize-uri": "^2.0.0",
    "micromatch": "^4.0.8",
    "ohash": "^1.1.4",
    "pathe": "^1.1.2",
    "remark-mdc": "^3.2.1",
    "scule": "^1.3.0",
    "shiki": "^1.22.2",
    "slugify": "^1.6.6",
    "tar": "^7.4.3",
    "typescript": "^5.6.3",
    "ufo": "^1.5.4",
    "unified": "^11.0.5",
    "unist-util-stringify-position": "^4.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5",
    "zod-to-ts": "^1.2.0"
  },
  "peerDependencies": {
    "pg": "*",
    "@libsql/client": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@nuxt/content": "link:.",
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.6.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.159",
    "@nuxt/test-utils": "^3.14.4",
    "@types/better-sqlite3": "^7.6.11",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.13",
    "csvtojson": "^2.0.10",
    "eslint": "^9.14.0",
    "micromark-util-types": "^2.0.0",
    "nuxt": "^3.14.159",
    "release-it": "^17.10.0",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10",
    "wrangler": "^3.85.0"
  },
  "resolutions": {
    "@nuxt/content": "workspace:*",
    "@nuxt/ui-pro": "https://pkg.pr.new/@nuxt/ui-pro@3623989",
    "vue": "^3.5.12"
  },
  "packageManager": "pnpm@9.12.3"
}
